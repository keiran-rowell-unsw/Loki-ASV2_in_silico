bg white

orient
center

spectrum b, orange_yellow_cyan_blue, minimum=50, maximum=90

# Backbone ribbon
cartoon automatic

set cartoon_fancy_helices, 1
set cartoon_tube_radius, 1

# Ray tracing 
set ray_trace_mode, 1
set ray_trace_gain, 0.05
set ray_trace_fog, 0
set ray_opaque_background, off
set ray_shadows, 1   # turn on shadows

set orthoscopic, off
set depth_cue=0
set antialias, 4
set hash_max, 300
ray 
